; Reverse Zone for 192.168.1.0/24 (INT)
; Generated by Ansible

$TTL    604800
@       IN      SOA     int-srv.{{ domain }}. admin.{{ domain }}. (
                              {{ ansible_date_time.date | replace('-', '') }}01
                              604800
                              86400
                              2419200
                              604800 )

@       IN      NS      int-srv.{{ domain }}.

; PTR Records
10      IN      PTR     int-srv.{{ domain }}.
254     IN      PTR     fw.{{ domain }}.
